[user]
	name = KIMURA Tetsuro
	email = haribote.nobody@gmail.com
[core]
	excludesfile = /Users/haribote/.gitignore_global
[color]
	ui = auto
	status = auto
	diff = auto
	branch = auto
[alias]
	ck = checkout
	cm = commit -m
	mg = merge --no-ff
	br = branch
	amend = commit --amend -C HEAD
	st = status
	graph = git log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	a = add .
	pk = cherry-pick
[pager]
        log = /usr/local/share/git-core/contrib/diff-highlight/diff-highlight | less
        show = /usr/local/share/git-core/contrib/diff-highlight/diff-highlight | less
        diff = /usr/local/share/git-core/contrib/diff-highlight/diff-highlight | less

